<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>cyber-net</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>cyber-net</name>
            <properties>
                <property name="ant.script">source/build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>source\src</label>
                    <type>java</type>
                    <location>source/src</location>
                    <encoding>windows-1251</encoding>
                </source-folder>
                <source-folder>
                    <label>cyber-net</label>
                    <location>.</location>
                    <encoding>windows-1251</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>buildlab</target>
                </action>
                <action name="javadoc">
                    <script>${ant.script}</script>
                    <target>javadoc</target>
                </action>
                <action name="test">
                    <script>${ant.script}</script>
                    <target>test</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>source\src</label>
                        <location>source/src</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>source/src</package-root>
                <classpath mode="compile">source/lib/dom4j.jar;source/lib/jaxen.jar</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
